<nav aria-label="Page navigation ">
  <ul class="pagination mb-0 justify-content-center">
    <li class="page-item">
      <a
        class="page-link p-3 icofont-arrow-left"
        href="#"
        aria-label="Previous"
        aria-hidden="true"
      >
      </a>
    </li>
    <% for(let i = 0 ; i < num ; i++ ) { %>
    <li class="page-item">
      <% if(current === i+1){ %>
      <a class="page-link px-4 py-2 active"><%= i+1 %></a>
      <% }else{ %>
      <a
        class="page-link px-4 py-2"
        href="#"
        ><%= i+1 %></a
      >
      <% } %>
    </li>
    <% } %>
    <li class="page-item">
      <a
        class="page-link p-3 icofont-arrow-right"
        href="#"
        aria-label="Next"
        aria-hidden="true"
      >
      </a>
    </li>
  </ul>
</nav>
